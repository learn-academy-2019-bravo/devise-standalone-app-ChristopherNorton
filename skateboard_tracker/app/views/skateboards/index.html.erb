<p id="notice"><%= notice %></p>

<h1>Skateboards</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model year</th>
      <th>Model</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skateboards.each do |skateboard| %>
      <tr>
        <td><%= skateboard.brand %></td>
        <td><%= skateboard.model_year %></td>
        <td><%= skateboard.model %></td>
        <td><%= skateboard.user_id %></td>
        <td><%= link_to 'Show', skateboard %></td>
        <td><%= link_to 'Edit', edit_skateboard_path(skateboard) %></td>
        <td><%= link_to 'Destroy', skateboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skateboard', new_skateboard_path %>
